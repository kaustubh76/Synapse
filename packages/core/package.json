{
  "name": "@synapse/core",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./llm": {
      "types": "./dist/llm/index.d.ts",
      "import": "./dist/llm/index.js",
      "require": "./dist/llm/index.js"
    },
    "./x402/*": {
      "types": "./dist/x402/*.d.ts",
      "import": "./dist/x402/*.js",
      "require": "./dist/x402/*.js"
    },
    "./x402": {
      "types": "./dist/x402/index.d.ts",
      "import": "./dist/x402/index.js",
      "require": "./dist/x402/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch"
  },
  "dependencies": {
    "@synapse/types": "*",
    "@synapse/mcp-x402": "*",
    "nanoid": "^5.0.4",
    "eventemitter3": "^5.0.1",
    "ethers": "^6.9.0"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
